    </link>

    <link name="thruster5">
      <pose>0 -@vert_thruster_y @vert_thruster_z 0 0 0</pose>
      <visual name="thruster_front">
        <pose>0 0 0.005 0 0 0</pose>
        <geometry>
          <box>
            <size>@propeller_size</size>
