        </material>
      </visual>
      <inertial>
        <mass>@propeller_mass</mass>
        <inertia>
          <ixx>@propeller_ixx</ixx>
          <ixy>0</ixy>
          <ixz>0</ixz>
          <iyy>@propeller_iyy</iyy>
          <iyz>0</iyz>
          <izz>@propeller_izz</izz>
        </inertia>
