      </inertial>
    </link>

    <!-- Attach left_camera_link to base_link -->
    <joint name="left_camera_joint" type="fixed">
      <!-- <pose>-0.15 0.18 -0.0675 0 1.5707963267948966 0</pose> -->
      <parent>base_link</parent>
      <child>left_camera_link</child>
    </joint>

