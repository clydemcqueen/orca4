          <iyy>0.001</iyy>
          <iyz>0</iyz>
          <izz>0.001</izz>
        </inertia>
      </inertial>
    </link>

    <link name="thruster5">
      <pose>0 -0.109 0.077 0 0 0</pose>
      <visual name="thruster_prop_visual">
        <pose>0 0 0 -1.5707963267948966 0 1.5707963267948966</pose>
        <geometry>
          <mesh>
