    </link>

    <link name="thruster4">
      <pose>-0.14 0.092 0 -1.5707963267948966 1.5707963267948966 2.356194490192345</pose>
      <visual name="thruster_prop_visual">
        <pose>0 0 0 1.5707963267948966 0 0</pose>
